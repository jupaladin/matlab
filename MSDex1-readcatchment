function varargout = Catchment_data(varargin)
% CATCHMENT_DATA MATLAB code for Catchment_data.fig
%      CATCHMENT_DATA, by itself, creates a new CATCHMENT_DATA or raises the existing
%      singleton*.
%
%      H = CATCHMENT_DATA returns the handle to a new CATCHMENT_DATA or the handle to
%      the existing singleton*.
%
%      CATCHMENT_DATA('CALLBACK',hObject,eventData,handles,...) calls the local
%      function named CALLBACK in CATCHMENT_DATA.M with the given input arguments.
%
%      CATCHMENT_DATA('Property','Value',...) creates a new CATCHMENT_DATA or raises the
%      existing singleton*.  Starting from the left, property value pairs are
%      applied to the GUI before Catchment_data_OpeningFcn gets called.  An
%      unrecognized property name or invalid value makes property application
%      stop.  All inputs are passed to Catchment_data_OpeningFcn via varargin.
%
%      *See GUI Options on GUIDE's Tools menu.  Choose "GUI allows only one
%      instance to run (singleton)".
%
% See also: GUIDE, GUIDATA, GUIHANDLES

% Edit the above text to modify the response to help Catchment_data

% Last Modified by GUIDE v2.5 14-Feb-2017 23:46:43

% Begin initialization code - DO NOT EDIT
gui_Singleton = 1;
gui_State = struct('gui_Name',       mfilename, ...
                   'gui_Singleton',  gui_Singleton, ...
                   'gui_OpeningFcn', @Catchment_data_OpeningFcn, ...
                   'gui_OutputFcn',  @Catchment_data_OutputFcn, ...
                   'gui_LayoutFcn',  [] , ...
                   'gui_Callback',   []);
if nargin && ischar(varargin{1})
    gui_State.gui_Callback = str2func(varargin{1});
end

if nargout
    [varargout{1:nargout}] = gui_mainfcn(gui_State, varargin{:});
else
    gui_mainfcn(gui_State, varargin{:});
end
% End initialization code - DO NOT EDIT


% --- Executes just before Catchment_data is made visible.
function Catchment_data_OpeningFcn(hObject, eventdata, handles, varargin)
% This function has no output args, see OutputFcn.
% hObject    handle to figure
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
% varargin   command line arguments to Catchment_data (see VARARGIN)

% Choose default command line output for Catchment_data
handles.output = hObject;

% Update handles structure
guidata(hObject, handles);

% UIWAIT makes Catchment_data wait for user response (see UIRESUME)
% uiwait(handles.figure1);


% --- Outputs from this function are returned to the command line.
function varargout = Catchment_data_OutputFcn(hObject, eventdata, handles) 
% varargout  cell array for returning output args (see VARARGOUT);
% hObject    handle to figure
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)

% Get default command line output from handles structure
varargout{1} = handles.output;


% --- Executes on button press in pushbutton1.
function pushbutton1_Callback(hObject, eventdata, handles)
% hObject    handle to pushbutton1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
% FILE OPEN/READ
File=fopen('Sieve.cal');
cname = fgetl(File);
i=1;
while isletter(cname(i))==0
    i=i+1;
end
Catchment_name = cname(i:length(cname));
fclose(File);

%Catchment_name=[F{3}{1},' ',F{4}{1},' ',F{5}{1}];

File=fopen('Sieve.cal');
F=textscan(File,'%s %s  %s %s  %s %s %s %s %s %s %s %s %s %s %s %s %s %s %s %s %s'); %Read the text file into the 1x 10 cell array F.  
fclose(File); % Close the text file

Catchment_area=[F{1}{2}];
Frq_Obsv=[F{1}{3}];
    if Frq_Obsv == '0'
        Frq_Obsv_txt = 'hours';
    elseif Frq_Obsv == '1'
        Frq_Obsv_txt = 'days';
    elseif Frq_Obsv == '2'
        Frq_Obsv_txt = 'weeks';
    elseif Frq_Obsv == '3'
        Frq_Obsv_txt = 'months';
    elseif Frq_Obsv == '4'
        Frq_Obsv_txt = 'unknown';
    end
    Pnum=[F{1}{9}];
set(handles.edit1,'String',Catchment_name); 
set(handles.edit2,'String',Catchment_area);
set(handles.edit3,'String',Frq_Obsv_txt);
set(handles.edit4,'String',Pnum);

% for image import
g=imread('rivers.jpg');
axes(handles.axes1);
imshow(g);



function edit1_Callback(hObject, eventdata, handles)
% hObject    handle to edit1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)

% Hints: get(hObject,'String') returns contents of edit1 as text
%        str2double(get(hObject,'String')) returns contents of edit1 as a double


% --- Executes during object creation, after setting all properties.
function edit1_CreateFcn(hObject, eventdata, handles)
% hObject    handle to edit1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    empty - handles not created until after all CreateFcns called

% Hint: edit controls usually have a white background on Windows.
%       See ISPC and COMPUTER.
if ispc && isequal(get(hObject,'BackgroundColor'), get(0,'defaultUicontrolBackgroundColor'))
    set(hObject,'BackgroundColor','white');
end



